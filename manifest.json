{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "0.3",
  "content_scripts": [
    {
      "matches": [
        "*://*.omaraha.ee/*/invest/stats/*"
      ],
      "exclude_matches": [
        "*://*.omaraha.ee/*/invest/stats/*total=true*"
      ],
      "js": [
        "or_stats.js"
      ]
    },
    {
      "matches": [
        "*://*.omaraha.ee/*/accounts/balance/*"
      ],
      "exclude_matches": [
        "*://*.omaraha.ee/*/accounts/balance/"
      ],
      "js": [
        "or_balance.js"
      ]
    },
    {
      "matches": [
        "*://*.omaraha.ee/*/invest/investments/*"
      ],
      "js": [
        "or_investments.js"
      ]
    }
  ],
  "default_locale": "en"
}