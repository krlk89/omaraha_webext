{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "0.5.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.omaraha.ee/*/invest/stats/*"
      ],
      "exclude_matches": [
        "*://*.omaraha.ee/*/invest/stats/*total=true*"
      ],
      "js": [
        "helper_s.js",
        "or_stats.js"
      ]
    },
    {
      "matches": [
        "*://*.omaraha.ee/*/invest/investments/*"
      ],
      "js": [
        "helper_s.js",
        "helper_l.js",
        "or_investments.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options/options.html"
  },
  "permissions": [
    "storage"
  ],
  "default_locale": "en"
}